from { transform: translate3d(0, 0, 0); }
to { transform: translate3d(-100%, 0, 0); visibility: hidden; }
}
@keyframes slideOutRight {
from { transform: translate3d(0, 0, 0); }
to { transform: translate3d(100%, 0, 0); visibility: hidden; }
}
