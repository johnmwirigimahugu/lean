    --on-error: #690005;
    --error-container: #93000a;
    --on-error-container: #ffb4ab;
    --background: #1c1b1f;
    --on-background: #e6e1e5;
    --surface: #1c1b1f;
    --on-surface: #e6e1e5;
    --surface-variant: #49454f;
    --on-surface-variant: #cbc4cf;
    --outline: #948f99;
  }

  /* High Contrast Theme */
  [data-theme="high-contrast"] {
    --primary: #000;
    --on-primary: #fff;
    --background: #fff;
    --on-background: #000;
    --surface: #fff;
    --on-surface: #000;
    --border: 2px solid #000;
    --shadow: none;
  }

  /* RTL Support */
  [dir="rtl"] { 
    direction: rtl; 
    text-align: right; 
  }
  
  /* Root Styles */
